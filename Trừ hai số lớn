#include <bits/stdc++.h>
using namespace std;
const int nm = 1e6 + 9;
const int N = 1e3 + 9;
const int MOD = 1e9 + 7;
const int LOG = 23;
#define el '\n'
#define pii pair<int,int>
string a,b; 
void equal_length(string &a, string &b){
	while (a.size() < b.size()) a = "0" + a; 
	while (b.size() < a.size()) b = "0" + b;
}

string tru(string &a, string &b){
	equal_length(a,b); 
	bool exc = false; 
	if (a < b) swap(a,b),exc = true; 
	string res = ""; int carry = 0; 
	for (int i = a.size()-1; i >= 0; i--){
		int d = (a[i] - '0') - (b[i] - '0') - carry; 
		if (d < 0){
			d += 10; 
			carry = 1; 
		}
		else carry = 0;
		res = (char)(d + '0') + res; 
	}
	while (res.size() > 1 && res.front() == '0') res.erase(res.begin()); 
	if (!exc) return res;
	else return "-" + res; 
}
signed main (){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	cin >> a >> b; 
	cout << tru(a,b);
	return 0;
}
