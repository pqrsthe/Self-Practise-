#include <bits/stdc++.h>
using namespace std;
const int nm = 1e6 + 9;
const int N = 1e3 + 9;
const int MOD = 1e9 + 7;
const int LOG = 23;
const long long INF = 2e9;
#define el '\n'
#define pii pair<int,int>
int k,x,ans=0; bool used[N]; 
bool check(int minn, int maxx){
	if (maxx >= minn*x) return false; return true; 
}

void backtrack(int j, int last, int minn, int maxx){
	if (j > k){
		if (check(minn,maxx)) ans++; 
		return; 
	}
	for (int i = 1; i <= k; i++){
		int nowmin = min(minn,i); 
		int nowmax = max(maxx,i); 
		backtrack(j+1,i,nowmin,nowmax); 
	}
}

signed main (){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	cin >> k >> x; 
	backtrack(1,1,INF,0); 
	cout << ans; 
	return 0;
}
