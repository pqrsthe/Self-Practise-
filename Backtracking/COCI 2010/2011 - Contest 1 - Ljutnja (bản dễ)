#include <bits/stdc++.h>
using namespace std;
const int nm = 1e6 + 9;
const int N = 1e3 + 9;
#define el '\n'
#define pii pair<int,int>
priority_queue<pii> pq; 
int m,n,a[nm]; 
map<int,int> mp; 
signed main (){
	freopen ("LJUTNJA.INP","r",stdin);
	freopen ("LJUTNJA.OUT","w",stdout);
	ios_base::sync_with_stdio(false);
	cin.tie(NULL); cout.tie(NULL);
	cin >> m >> n; 
	for (int i = 1; i <= n; i++){
		cin >> a[i]; pq.push({a[i],i}); 
	}
	while (m > 0){
//		cout << pq.top().first << " "; 
		pii cur = pq.top(); pq.pop(); 
		m--; pq.push(make_pair(cur.first - 1, cur.second)); 
	}
	int ans = 0; 
	while (!pq.empty()){
		int i = pq.top().second, value = pq.top().first; pq.pop(); 
		ans += (value)*value; 
	}
	cout << ans; 
	return 0;
}
