#include <bits/stdc++.h>
using namespace std;
#define el '\n'
#define ll long long
#define pb push_back
//const long long INF = 1e12;
const int N = 1e5 + 5;
const int M = 5e5 + 90;
const int MOD = 1e9 + 7;
const int MAX = 2e6 + 90;
const long long INF = 2e16;
int n,kmp[N]; string s;
void Input(){
    cin >> n;
    cin >> s;
    s = " " + s;
}
void KMP(){
    int k = kmp[1] = 0;
    for (int i = 2; i <= n; i++){
        while (k > 0 && s[i] != s[k+1]) k = kmp[k];
        kmp[i] = (s[i] == s[k+1]) ? ++k : 0;
    }
}
void solve(){
    KMP();
    vector<int> ans(n+1,1);
    for (int i = n; i >= 1; i--){
        if (kmp[i]) ans[kmp[i]] += ans[i];
    }
    for (int i = 1; i <= n; i++){
        cout<<"ans["<<i<<"]="<<ans[i]<<el;
    }
}
signed main(){
    freopen ("test.inp","r",stdin);
    freopen ("Test.out","w",stdout);
    ios_base::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);
    int t; t = 1;
    while (t--){
        Input();
        solve();
    }
    return 0;
}
